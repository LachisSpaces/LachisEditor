<?xml version="1.0" encoding="utf-8" ?>
<LachisEditor>
   <MainWindow Text="Lachi's Editor">
      <ApplicationMenu>
         <DBImport Text="Extraer cdb"/>
         <DBExport Text="Exportar a cdb"/>
         <DBOpen Text="Abrir Base de datos"/>
         <DBSave Text="Guardar Base de Datos"/>
         <DBClose Text="Close database"/>
         <LanguageSelection Text="Idiomas" ToolTip="Cambiar el idioma de la aplicación"/>
         <Options Text="Opciomes" ToolTip="Opciones Generales">
            <UseTranslatedFields Text="Traducir Columnas"/>
            <UseForeignKeyLookup Text="Mostrar listas en lugar de ID's"/>
            <UseTeamFilter Text="Filtro de equipo"/>
         </Options>
         <Exit Text="Salir"/>
      </ApplicationMenu>
      <TabStart Text="Incio">
         <Databases Text="Bases de Datos">
            <SelectDatebase Text="Seleccionar Base de Datos"/>
            <DeleteDatebase Text="Borrar Base de Datos"/>
         </Databases>
         <Tables Text="Tablas">
            <SelectTable Text="Seleccionar tabla"/>
            <DYN_manager Text="Mánager"/>
            <DYN_sponsor Text="Sponsor"/>
            <DYN_objectif Text="Objetivos"/>
            <DYN_team Text="Equipos"/>
            <DYN_cyclist Text="Ciclistas"/>
            <STA_race Text="Carreras"/>
            <STA_stage Text="Etapas"/>
         </Tables>
         <Filter Text="Filter">
            <Apply Text="Aplicar"/>
            <Clear Text="Limpiar"/>
         </Filter>
         <MassEdit Text="Cambio masivo">
            <Apply Text="Aplicar"/>
            <Clear Text="Limpiar"/>
         </MassEdit>
         <Layout Text="Layout">
            <DefineLayout Text="Crear un layout"/>
            <ResetPosition Text="Posición de las columnas predeterminada"/>
            <ResetSorting Text="Grupos predeterminados"/>
            <ResetSize Text="Tamaño de columna predeterminado"/>
         </Layout>
         <LayoutSubsets Text="Grupos de Layout">
            <Layout0 Text="No existen layouts"/>
            <Layout1 Text="Layout 1"/>
            <Layout2 Text="Layout 2"/>
         </LayoutSubsets>
      </TabStart>
      <Messages>
         <NoDataLoaded Text="Primero necesita abrir o extraer una base de datos."/>
         <DatabaseLoadedAlready Text="Ya ha abierto una base de datos."/>
         <NoRecentDatabase Text="Por favor, extraiga una base de datos."/>
         <LoadingWaiting Text="Por favor, espere hasta que la base de datos se haya cargado."/>
         <SecurityCheckUnsavedData Text="Quieres guardar antes de cerrar la base de datos?" />
         <FileNameInvalid Text="Nombre de archivo no válido. Por favor, elimine todos los espacios del nombre."/>
         <MassEditWarning Text="Nota, este cambio no se puede revertir.\r\nPuede guardar antes, si es necesario.\r\n\r\nUtilice el filtro si no desea alterar todas las filas."/>
      </Messages>
   </MainWindow>
   <LayoutDefinition Text="Edición de layout">
      <Buttons>
         <MoveUp Text="Mover hacia arriba"/>
         <MoveUp5 Text="Mover 5 posiciones hacia arriba"/>
         <MoveUp10 Text="Mover 10 posiciones hacia arriba"/>
         <MoveDown Text="Mover hacia abajo"/>
         <MoveDown5 Text="Mover 5 posiciones hacia abajo"/>
         <MoveDown10 Text="Mover 10 posiciones hacia abajo"/>
         <MoveFirst Text="Mover al inicio"/>
         <MoveLast Text="Move al final"/>
         <Cancel Text="Cancelar"/>
         <Apply Text="Aceptar"/>
      </Buttons>
      <Columns>
         <FieldName Text="Nombre de Columna"/>
         <Width Text="Anchura"/>
         <Column Text="Columna"/>
         <LayoutGroups Text="Grupos"/>
      </Columns>
   </LayoutDefinition>
   <DBLoader>
      <LoadDatabaseNoData Text="No se ha encontrado ninguna base de datos guardada, por favor utilice 'Nuevo plan'."/>
      <DeleteFitnessProgram Text="Algunos programas de forma ya existen.\r\nQuieres borrarlos antes de crear unos nuevos?\r\nCuidado: Los programas se pueden sobreescribir al crear el nuevo plan."/>
      <ExtractDB_NumberInvalid Text="Hay un error en la tabla  {0}  en la fila  {1} = {2}.\r\nEl valor de la columna  {3}  se ha restablecido a 0.\r\nEl valor corrupto fue {4}.\r\n\r\nTiene que comprobar y corregir los datos de esta fila."/>
   </DBLoader>

   <DYN_cyclist>
      <gene_sz_lastname Text="Apellido"/>
      <gene_sz_firstname Text="Nombre"/>
      <value_f_current_evaluation Text="MED"/>
   </DYN_cyclist>
   <STA_race>
      <gene_sz_race_name Text="Carrera"/>
   </STA_race>
   <STA_stage>
      <gene_sz_stage_name Text="Nombre de la Etapa"/>
   </STA_stage>
</LachisEditor>

